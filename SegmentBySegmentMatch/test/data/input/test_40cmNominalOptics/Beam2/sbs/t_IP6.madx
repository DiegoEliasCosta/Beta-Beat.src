twiss, file="__PATH__Beam2/sbs//StartPoint.twiss";

!use, period=LHCB2;
!select, flag=twiss, clear;
!select, flag=twiss, range=BPM.15R6.B2, column=name,s,betx,alfx,bety,alfy,mux,muy, dx, r11,r12, r22,r21, x,y,dx,dy,dpx,dpy;
!twiss, file="__PATH__Beam2/sbs//EndPoint.twiss";


