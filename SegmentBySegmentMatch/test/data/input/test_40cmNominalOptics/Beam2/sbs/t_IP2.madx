twiss, file="__PATH__Beam2/sbs//StartPoint.twiss";

!use, period=LHCB2;
!select, flag=twiss, clear;
!select, flag=twiss, range=BPM.15R2.B2, column=name,s,betx,alfx,bety,alfy,mux,muy, dx, r11,r12, r22,r21, x,y,dx,dy,dpx,dpy;
!twiss, file="__PATH__Beam2/sbs//EndPoint.twiss";


!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!mathing
!call, file="__PATH__Beam2/sbs//matchjob4_IP2_sbs.madx";
!!!!!!!!!!! end matching



!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!! normal propogation


!!
!!  Start at the first BPM
!!

seqedit, sequence=LHCB2; 
cycle, start=BPM.15L2.B2;
endedit;



!!
!! Save initial beta in beta0 variable
!!

use, period=LHCB2, range=BPM.15L2.B2/BPM.15L2.B2;  ! Just the starting element for speed
savebeta,label=b02, place=BPM.15L2.B2;
twiss ,chrom, betx=30.6343476268, alfx=0.470060419472, bety=170.538629972, alfy=-2.34744292579,dx=0,dy=0,dpx=0,dpy=0, wx=0, phix=0, wy=0, phiy=0;

!!
!!!! Matching for coupling initial cond, R matrix
!!

select, flag=twiss, clear;
select, flag=twiss, pattern=BPM ,column=name,s,betx,alfx,bety,alfy,mux,muy, dx, r11,r12, r22,r21, x,y,dx,dy,dpx,dpy, wx, phix, wy, phiy;

MATCH,USE_MACRO;
VARY, name=ir11, step=0.00001;
VARY, name=ir12, step=0.00001;
VARY, name=ir21, step=0.00001;
VARY, name=ir22, step=0.00001;

M1: MACRO={  
    savebeta,label=b2,place=BPM.15L2.B2;  ! This will update b1 with all the betas and coupling input
    twiss,beta0=b02, chrom, R11=ir11,R12=ir12,R21=ir21,R22=ir22,file="__PATH__Beam2/sbs//twiss_IP2.dat";
    system, "/usr/bin/python __PATH__Beam2/sbs//getfterms_0.3.py -p __PATH__Beam2/sbs// -l IP2 -f 1 -m _free";
    call, file="__PATH__Beam2/sbs//initvals.dat";
}

