!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!! normal propogation


!!
!!  Start at the first BPM
!!

seqedit, sequence=LHCB1; 
cycle, start=BPM.15L2.B1;
endedit;



!!
!! Save initial beta in beta0 variable
!!

use, period=LHCB1, range=BPM.15L2.B1/BPM.15L2.B1;  ! Just the starting element for speed
savebeta,label=b0, place=BPM.15L2.B1;
twiss ,chrom, betx=156.571040625, alfx=-2.22664645674, bety=33.0737403149, alfy=0.704737240335,dx=0,dy=0,dpx=0,dpy=0, wx=0, phix=0, wy=0, phiy=0;

!!
!!!! Matching for coupling initial cond, R matrix
!!

select, flag=twiss, clear;
select, flag=twiss, pattern=BPM ,column=name,s,betx,alfx,bety,alfy,mux,muy, dx, r11,r12, r22,r21, x,y,dx,dy,dpx,dpy, wx, phix, wy, phiy;

MATCH,USE_MACRO;
VARY, name=ir11, step=0.00001;
VARY, name=ir12, step=0.00001;
VARY, name=ir21, step=0.00001;
VARY, name=ir22, step=0.00001;

M1: MACRO={  
    savebeta,label=b1,place=BPM.15L2.B1;  ! This will update b1 with all the betas and coupling input
    twiss,beta0=b0, chrom, R11=ir11,R12=ir12,R21=ir21,R22=ir22,file="__PATH__Beam1/sbs//twiss_IP2.dat";
    system, "/usr/bin/python __PATH__Beam1/sbs//getfterms_0.3.py -p __PATH__Beam1/sbs// -l IP2 -f 1 -m _free";
    call, file="__PATH__Beam1/sbs//initvals.dat";
}

